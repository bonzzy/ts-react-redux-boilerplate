{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "max-line-length": [true, 120],
    "variable-name": [false],
    "align": [true, "statements", "members", "elements"],
    "strict-boolean-expressions": false,
    "ter-indent": [true, 4]
  },
  "rulesDirectory": []
}